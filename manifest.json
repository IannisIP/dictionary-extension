{
    "manifest_version": 2,
    "name": "Dictionary extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["/js/contentscript.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "DOM html elements counter."
    }
}